{
    "version": 2,
    "builds": [
        {
            "src": "app.py", 
            "use": "@vercel/python"
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build"
        },
        {
            "src": "api/**/*",
            "use": "@vercel/python"
        }
    ],
    "routes":[
        {"src": "/(.*)", "dest": "app.py"}
    ]
}
